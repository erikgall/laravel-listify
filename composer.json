{
    "name": "erikgall/listify",
    "description": "Turn any Eloquent model into a list! http://erikgall.github.io/listify",
    "keywords": ["laravel", "model", "position", "acts_as_list", "rails", "list", "eloquent", "order"],
    "license": "MIT",
    "authors": [{
        "name": "Erik Galloway",
        "email": "egalloway@claruscare.com"
    }],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/database": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/events": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "EGALL\\Listify\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/models"]
    },
    "minimum-stability": "stable",
    "config": {
        "discard-changes": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "EGALL\\Listify\\ListifyServiceProvider"
            ]
        }
    }
}
